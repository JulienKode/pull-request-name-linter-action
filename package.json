{
  "name": "pull-request-commitlint-action",
  "version": "19.0.0",
  "private": true,
  "description": "Run commitlint on pull request name.",
  "main": "lib/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "bundle": "node esbuild.config.js",
    "test": "jest",
    "build:pack": "yarn build && yarn bundle",
    "all": "yarn build && yarn test && yarn bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Julien Karst",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/types": "20.0.0",
    "@types/jest": "27.5.2",
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "0.38.1",
    "esbuild": "^0.25.8",
    "eslint": "8.56.0",
    "eslint-plugin-github": "3.4.1",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "27.5.1",
    "jest-circus": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "27.1.5",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/github": "5.1.1",
    "@commitlint/lint": "20.0.0",
    "@commitlint/load": "20.1.0"
  },
  "resolutions": {
    "import-fresh": "JulienKode/import-fresh#fix/issue-when-there-no-parent-module"
  }
}
